{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["/^versions.go$/"],
      "datasourceTemplate": "go",
      "matchStrings": [
          "// renovate: (?<depName>.*?)"
      ]
    }
  ],
  "gomod": {
    "groupName": "Go dependencies",
  }
}